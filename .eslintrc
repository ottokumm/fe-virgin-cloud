{
  "parser": "babel-eslint",
  "extends": [
    "airbnb"
  ],
  "globals": {
    "document": false
  },
  "rules": {
    "react/prop-types": "off",
    "react/jsx-closing-bracket-location": [
      1,
      {
        "selfClosing": "line-aligned",
        "nonEmpty": "after-props"
      }
    ],
    "import/prefer-default-export": "off",
    "no-use-before-define": [
      "error",
      {
        "functions": false
      }
    ],
    "indent": "off",
    "react/jsx-indent": "off",
    "react/jsx-indent-props": "off",
    "no-console": [
      "warn",
      {
        "allow": [
          "warn"
        ]
      }
    ]
  }
}
